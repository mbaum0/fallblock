{
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/fallblock",
            "args": ["multi", "client"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "(lldb) Launch - One",
            "program": "${workspaceFolder}/build/fallblock",
            "args": ["multi", "3333", "3334"],
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "(lldb) Launch - Two",
            "program": "${workspaceFolder}/build/fallblock",
            "args": ["multi", "3334", "3333"],
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
        }
    ],
    "version": "2.0.0"
}